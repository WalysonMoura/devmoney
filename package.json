{
  "name": "fastify",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "migrate:latest": "knex migrate:latest",
    "lint": "eslint src --ext .ts --fix",
    "build": "tsup src --dts --outdir=build/",
    "test": "vitest"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "dotenv": "^16.4.4",
    "fastify": "^4.26.1",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^20.11.19",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
